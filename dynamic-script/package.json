{
  "name": "@kleros/cross-chain-realitio-dynamic-script",
  "version": "0.5.0",
  "description": "Dynamic script for cross-chain Reality.eth integration",
  "author": "Kleros",
  "license": "MIT",
  "type": "module",
  "main": "./dist/realitio-dynamic-script.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/realitio-dynamic-script.js",
      "types": "./dist/index.d.ts"
    },
    "./lib": {
      "import": "./dist/lib.js",
      "types": "./dist/lib.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "lib": ["./dist/lib.d.ts"]
    }
  },
  "files": ["dist", "src"],
  "scripts": {
    "clean": "rm -rf dist",
    "build:types": "tsc --emitDeclarationOnly --project tsconfig.json",
    "build": "yarn clean && vite build && yarn build:types",
    "format": "biome format --write ./src",
    "lint": "biome lint ./src",
    "check": "biome check --write ./src"
  },
  "dependencies": {
    "@reality.eth/reality-eth-lib": "^3.3.2",
    "viem": "^2.23.13"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^20.11.24",
    "typescript": "^5.8.2",
    "vite": "^5.4.14"
  },
  "volta": {
    "node": "20.19.0"
  }
}
